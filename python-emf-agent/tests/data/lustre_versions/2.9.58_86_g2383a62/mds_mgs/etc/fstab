/dev/mapper/vg_00-lv_root /                       ext4    defaults        1 1
UUID=2132a43c-883d-47b6-a87b-da65edda24c9 /boot                   ext3    defaults        1 2
/dev/mapper/vg_00-lv_swap swap                    swap    defaults        0 0
