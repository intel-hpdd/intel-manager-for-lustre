[Unit]
Description=EMF SFA Service
PartOf=emf-manager.target
After=postgresql-9.6.service
After=emf-settings-populator.service
Requires=emf-settings-populator.service


[Service]
Type=simple
Environment=RUST_LOG=info
EnvironmentFile=/var/lib/chroma/emf-settings.conf
EnvironmentFile=-/var/lib/chroma/overrides.conf
ExecStart=/bin/emf-sfa
Restart=on-failure
RestartSec=2
StandardOutput=journal
StandardError=journal